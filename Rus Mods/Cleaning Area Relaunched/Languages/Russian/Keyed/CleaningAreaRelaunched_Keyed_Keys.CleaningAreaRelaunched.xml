	<!-- EN: Enable debug logging -->
	<CAR_DebugLoggingEnabled>Включить журнал отладки</CAR_DebugLoggingEnabled>
	<!-- EN: Lots of messages yay -->
	<CAR_DebugLoggingEnabledTooltip>Куча сообщений в логе, ура.</CAR_DebugLoggingEnabledTooltip>
